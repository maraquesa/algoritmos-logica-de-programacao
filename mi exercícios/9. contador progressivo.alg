algoritmo "contador crescente"
var
   valor, contador: inteiro
inicio
      contador <- 0
      Escreva ("Quer contar ate quanto? ")
      leia (valor)
      Enquanto (contador <= valor)faca
               EscrevaL (contador)
               contador <- contador + 1
      FimEnquanto
      EscrevaL ("Terminei de Contar")
fimalgoritmo
