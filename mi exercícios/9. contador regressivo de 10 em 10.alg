algoritmo "contador decrescente de 10 em 10"
var
   cont, valor: inteiro
inicio
      Escreva ("Quer contar a partir de quanto? (multiplo de 10) ")
      Leia (valor)
      cont <- valor
      Enquanto (cont >= 0) faca
               EscrevaL (cont)
               cont <- cont - 10
      FimEnquanto
fimalgoritmo
